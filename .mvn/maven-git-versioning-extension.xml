<?xml version="1.0" encoding="UTF-8"?>
<gitVersioning>
  <branch>
    <pattern>.*</pattern>
    <versionFormat>${branch}-SNAPSHOT</versionFormat>
  </branch>
  <tag>
    <pattern>v([0-9].*)</pattern>
    <versionFormat>${1}</versionFormat>
  </tag>
  <commit>
    <versionFormat>${commit}</versionFormat>
  </commit>
</gitVersioning>
